							<div class="progress">
								<div
									class="progress-bar bg-danger"
									role="progressbar"
									style="width: 25%"
									aria-valuenow="25"
									aria-valuemin="0"
									aria-valuemax="100"
								>
									25%
								</div>
							</div>
							<div class="option-votes py-3">
